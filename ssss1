#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;
int main()
{
	int n, a, b, x, s, f, d;
	s = 0;
	f = 0;
	cout << "n=";
	cin >> n;
	cout << "a=" << "b=";
	cin >> a >> b;
	d = a;
	while (a <= b)
	{
		cout << a;
		a++;
	}
	cout << endl;
	while (n > 0)
	{
		cout << "x=";
		cin >> x;
		if ((x >= d) && (x <= b))
		{
			s = s + 1;
			n--;
		}
		else
		{
			f = f + x;
			n--;
		}
	}
	cout << s << endl;
	cout << f << endl;
	return 0;
}
