#include <iostream>
#include <iomanip>
using namespace std;

int main() {
	int n, k1 = 0, k2 = 0;
	cin >> n;
	int** a = new int* [n];
	for (int i = 0; i < n; i++)
	{
		a[i] = new int[n];
		for (int j = 0; j < n; j++)
			cin >> a[i][j];
	}

	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < n; j++)
			cout << setw(3) << a[i][j];
		cout << endl;
	}	
	cout << endl;
	int* A = new int[n];
	for (int i = 0; i < n; i++)
	{
		A[i] = 1;
	}
	cout << 'k1=';
	cin >> k1;
	cout << "k2=";
	cin >> k2;
	for (int j = 0; j < n; j++)
	{
		for (int i = k1; i <= k2; i++)
		{
			A[j] *= a[j][i];
		}
	}
		for (int i = 0; i < n; i++)
		{
			cout << "Product of " << i << "th column:" << A[i] << '\n';
		}
	return 0;
}
