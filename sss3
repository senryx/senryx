#include <iostream>
#include <iomanip>
#include <cmath>
using namespace std;
int main()
{
    cout << fixed << setprecision(6);
    int n;
    double x, p;
    p = 1;
    cout << "Enter n" << endl;
    cin >> n;
    while (n > 0)
    {
        cin >> x;
            p = p * x;
            n--;
    }
    cout << "Answer = " << p << endl;
}
